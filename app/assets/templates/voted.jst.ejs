<% if (vote.upvote) { %>

	<div class="guess-upvote">
		<span class="icon-thumbs-up vote upvoted" data-id="<%=vote.id%>" data-guess-id="<%=vote.guess_id%>"> </span>
	</div>
	<div class="guess-downvote">
		<span class="icon-thumbs-down vote" data-id="<%=vote.id%>" data-guess-id="<%=vote.guess_id%>"></span>
	</div>

<% } else { %>
<!-- ACCOUNT FOR AFTER VOTING GIVING THE OTHER BUTTON A VOTE ID -->
	<div class="guess-upvote">
		<span class="icon-thumbs-up vote" data-id="<%=vote.id%>" data-guess-id="<%=vote.guess_id%>"></span>
		</div>
	<div class="guess-downvote">
		<span class="icon-thumbs-down vote downvoted" data-id="<%=vote.id%>" data-guess-id="<%=vote.guess_id%>"></span>
	</div>

<% } %>